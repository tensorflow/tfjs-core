{
  "name": "deeplearn-tf-demo",
  "version": "0.0.1",
  "description": "Import Tensorflow models and running in deeplearn.js",
  "main": "dist/tfdemo/index.js",
  "unpkg": "dist/bundle.js",
  "types": "dist/tfdemo/index.d.ts",
  "peerDependencies": {
    "deeplearn-tensorflow": "0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PAIR-code/deeplearnjs.git"
  },
  "devDependencies": {
    "browserify": "~14.5.0",
    "browserify-shim": "~3.8.14",
    "deeplearn": "0.3.16",
    "mkdirp": "~0.5.1",
    "tsify": "~3.0.3",
    "tslint": "~5.8.0",
    "typescript": "~2.6.1",
    "uglifyjs": "~2.4.11",
    "watchify": "~3.9.0"
  },
  "scripts": {
    "preinstall": "cd .. && yarn prep && yarn build && cd ./demo && npm install ../",
    "prep": "yarn && mkdirp dist",
    "build": "tsc --sourceMap false && browserify -g browserify-shim --standalone tfdemo index.ts -p [tsify] -o dist/bundle.js",
    "lint": "tslint -p . -t verbose",
    "publish-npm": "yarn build && yarn publish"
  },
  "browserify-shim": {
    "deeplearn": "global:dl"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/long": "^3.0.32",
    "dag-iterator": "^0.3.0",
    "deeplearn": "^0.3.14",
    "deeplearn-tensorflow": "file:..",
    "protobufjs": "~6.8.0"
  }
}
